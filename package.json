{
  "name": "flowgrid",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "cd packages/prisma && prisma generate",
    "db:push": "cd packages/prisma && prisma db push",
    "db:seed": "cd packages/prisma && tsx seed.ts"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "tsx": "^4.19.0",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@prisma/client": "^7.0.0"
  }
}
